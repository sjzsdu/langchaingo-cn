{
  "llms": {
    "chain_llm": {
      "type": "kimi",
      "model": "moonshot-v1-8k",
      "api_key": "${KIMI_API_KEY}",
      "base_url": "",
      "temperature": 0.7,
      "max_tokens": null,
      "options": null
    }
  },
  "memories": {
    "chain_memory": {
      "type": "conversation_buffer",
      "max_token_limit": null,
      "max_messages": null,
      "return_messages": null,
      "llm_ref": "",
      "options": null
    }
  },
  "prompts": {
    "chain_prompt": {
      "type": "prompt_template",
      "template": "你是一个专业的AI助手，请用中文回答用户问题：{{.input}}",
      "input_variables": [
        "input"
      ],
      "partial_variables": null,
      "template_format": "",
      "validate_template": null,
      "messages": null,
      "options": null
    }
  },
  "chains": {
    "main_chain": {
      "type": "conversation",
      "llm_ref": "chain_llm",
      "memory_ref": "chain_memory",
      "prompt_ref": "chain_prompt",
      "chains": null,
      "input_keys": null,
      "output_keys": null,
      "separator": "",
      "max_concurrency": null,
      "options": null
    }
  }
}