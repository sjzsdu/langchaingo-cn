{
  "agent": {
    "type": "zero_shot_react",
    "chain": {
      "type": "llm",
      "llm": {
        "type": "deepseek",
        "model": "deepseek-chat",
        "api_key": "${DEEPSEEK_API_KEY}",
        "temperature": 0.7
      }
    },
    "output_key": "result"
  },
  "memory": {
    "type": "conversation_buffer",
    "max_messages": 5,
    "return_messages": true
  },
  "max_iterations": 5,
  "return_intermediate_steps": false
}