{
  "agent": {
    "type": "conversational_react",
    "chain": {
      "type": "llm",
      "llm": {
        "type": "deepseek",
        "model": "deepseek-chat",
        "api_key": "${DEEPSEEK_API_KEY}",
        "temperature": 0.7,
        "max_tokens": 2000
      },
      "prompt": {
        "type": "chat_prompt_template",
        "messages": [
          {
            "role": "system",
            "template": "You are a helpful AI assistant. Think step by step and use tools when needed."
          },
          {
            "role": "human",
            "template": "{input}"
          }
        ],
        "input_variables": ["input", "agent_scratchpad"]
      },
      "memory": {
        "type": "conversation_buffer",
        "max_messages": 10,
        "return_messages": true
      }
    },
    "output_key": "output",
    "options": {
      "tools": ["calculator", "web_search"],
      "verbose": true,
      "max_steps": 5
    }
  },
  "memory": {
    "type": "conversation_summary",
    "max_token_limit": 4000,
    "llm": {
      "type": "qwen",
      "model": "qwen-turbo",
      "api_key": "${QWEN_API_KEY}",
      "temperature": 0.3
    }
  },
  "max_iterations": 10,
  "return_intermediate_steps": true
}
